{
    "name": "portfolio-app",
    "main": "expo-router/entry",
    "version": "1.0.0",
    "scripts": {
        "build-css": "sass styles/index.scss styles/index.css --watch",
        "start": "expo start --dev-client -c",
        "reset-project": "node ./scripts/reset-project.js",
        "android": "expo prebuild && ENVFILE=.env expo run:android",
        "ios": "ENVFILE=.env expo run:ios",
        "web": "expo start --web",
        "test": "jest --watchAll",
        "lint": "expo lint",
        "build-android": "ENVFILE=.env eas build -p android --profile preview",
        "build-local": "expo prebuild && eas build --local"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "expo": {
        "doctor": {
            "reactNativeDirectoryCheck": {
                "listUnknownPackages": false
            }
        }
    },
    "dependencies": {
        "@expo-google-fonts/inter": "^0.2.3",
